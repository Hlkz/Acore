[clientmanager]

# Global

FullDataPath = "D:\A\Client\Manager\Data\FullData"
LocDataPath = "D:\A\Client\Manager\Data\LocData"
TinyDataPath = "D:\A\Client\Manager\Data\TinyData"
TinyLocPath = "D:\A\Client\Manager\Data\TinyLoc"
TinyPatchPath = "D:\A\Client\Manager\Data\TinyPatch"

DataPath = "D:\A\Client\Manager\Data"
UpdatePath = "D:\A\Client\Manager\Update"

# ClientCompressor

PatchOutputPath = "D:\A\Client\Manager\Output"
ReleasePath = "D:\A\Client\Manager\Release"
GameDataPath = "D:\A\Client\Aviana\Data"
ClientName = "Aviana"
RealmlistAddress = "auth.aviana-online.com"

# ClientPatcher

AnnounceAddress = "http://localhost/announce"
PatchAddress = "http://localhost/update/patch"
PatchNoteAddress = "http://localhost/patch-note"
PatchReleasePath = "D:\A\Build\bin\Release\data\patches"

# Database

LoginDatabaseInfo = "127.0.0.1;3306;root;;iwpa"
UnusedDatabaseInfo = "127.0.0.1;3306;root;;iwpu"
WorldDatabaseInfo = "127.0.0.1;3306;root;;iwpw"

# ClientPatcher - Version

NewClientBuild = 30000
NewMajorVersion = 3
NewMinorVersion = 0
NewBugfixVersion = 0
NewHotfixVersion = ""

# Logging

LogsDir = "./log"

Appender.Console=1,3,0
Appender.Manager=2,2,0,Manager.log,w
Appender.Extract=2,2,8,ManagerExtract.log,w
Appender.ExtractExist=2,2,8,ManagerExtractExist.log,w
Appender.ExtractLookup=2,2,8,ManagerExtractLookup.log,w

Logger.root=5,Console Manager
Logger.client.selector=3,Console Manager
Logger.client.extract=3,Extract
Logger.client.extract.exist=3,ExtractExist
Logger.client.extract.lookup=3,ExtractLookup
